{
  "name": "email-preview",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook & yarn backend",
    "build": "gulp",
    "build:addons": "tsc --project ./addons/tsconfig.json",
    "build:addons:watch": "yarn build:addons --watch",
    "build:backend": "tsc --project ./server/tsconfig.json",
    "build:backend:watch": "yarn build:backend --watch",
    "deploy": "firebase deploy --only hosting",
    "backend": "node ./server/build/index.js",
    "backend:development": "node ./server/build/index.js --development",
    "backend:development:watch": "nodemon ./server/build/index.js --development",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "storybook:build": "build-storybook",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@storybook/addon-a11y": "^6.5.14",
